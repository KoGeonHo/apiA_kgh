<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
<display-name>PlayNet3</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>FrontController</servlet-name>
    <servlet-class>playnet.common.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontController</servlet-name>
    <url-pattern>*.go</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <servlet-name>FrontQueryAction</servlet-name>
    <servlet-class>playnet.common.FrontQueryAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontQueryAction</servlet-name>
    <url-pattern>*.ok</url-pattern>
  </servlet-mapping>
</web-app>